{
  "name": "@double-cup/express-actuator",
  "version": "0.0.1",
  "description": "Swagger middleware and mocks for Express",
  "keywords": [
    "express",
    "swagger",
    "middleware",
    "mock",
    "fake",
    "stub",
    "rest",
    "api",
    "json"
  ],
  "author": {
    "name": "Julien Andreu <julienandreu@me.com>",
    "url": "https://julienandreu.com"
  },
  "homepage": "https://github.com/double-cup/express-actuator",
  "repository": {
    "type": "git",
    "url": "https://github.com/double-cup/express-actuator.git"
  },
  "license": "MIT",
  "main": "lib/index.js",
  "files": [
    "lib",
    "samples"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "clean": "shx rm -rf coverage",
    "lint": "eslint .",
    "test": "jest && npm run lint",
    "coverage": "jest --coverage",
    "upgrade": "npm-check -u && npm audit fix",
    "bump": "bump --tag --push --all",
    "release": "npm run upgrade && npm run clean && npm test && npm run bump",
    "start": "cd samples && node sample1.js"
  },
  "devDependencies": {
    "eslint": "^6.8.0",
    "eslint-config-google": "^0.14.0",
    "express": "^4.17.1",
    "npm-check": "^5.9.0",
    "shx": "^0.3.2",
    "jest": "latest"
  },
  "dependencies": {
    "uuid": "^7.0.3"
  },
  "peerDependencies": {
    "express": "4.x"
  }
}
